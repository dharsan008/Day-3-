package Day3;

import java.util.Scanner;

public class ifelseif {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int a= sc.nextInt();
		if(a % 3==0  && a % 5==0)
		{
			System.out.println("fizz bizz");
		}
		else if(a % 5==0)
		{
			System.out.println("bizz");
		}
		else if(a % 3==0)
		{
			System.out.println("fizz ");
		}
	}

}
